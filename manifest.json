{
  "manifest_version": 2,
  "name":    "14 OSS Skeleton",
  "version": "1.0",
  "description": "Get the input field value from the web",
  
  "background": {
    "scripts": ["background.js"]
  },
  
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["jquery-1.11.1.min.js", "myscript.js"],
      "run_at": "document_start"
    }
  ],
  
  "permissions": [
    "<all_urls>",
    "tabs",
    "storage"
  ],

  "browser_action": {
	  "default_title": "SecureInfo",
	  "default_popup": "popup.html"
  }
}
